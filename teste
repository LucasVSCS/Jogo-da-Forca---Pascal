  writeln('   /MMMMMMMM                     :dMMMMMo        ');
  writeln('   .NMMMMMMMmy:                    :hMMMo        ');
  writeln('    `/hMMMMMMMMd.                    mMMo        ');
  writeln('     :NMMMMMMMMMN-                   mMMo        ');
  writeln('    :MMMMMMMMMNMMm                   mMMo        ');
  writeln('       yNMMMMM/                      mMMo        ');
  writeln('       `NMMMMM:                      mMMo        ');
  writeln('        NMMMMM:                      mMMo        ');
  writeln('        NMMMMM-                      mMMo        ');
  writeln('                                     NMMs        ');
  writeln('                                    yMMMM/       ');
  writeln('                                   sMMMMMM/      ');
  writeln('                                  oMMMMMMMM/     ');
  writeln('                                 /MMMMMMNMMM:    ');
  writeln('                                :MMMomMMosMMM:   ');
