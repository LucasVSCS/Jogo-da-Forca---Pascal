procedure merda();
begin
  writeln
  readln (palavra);
  if (palavra <> resposta[opcao]) then
  begin
    boneco_cabeca()
  end;
  readln (palavra)
  clrscr;
  if (palavra <> resposta[opcao]) then
  begin
    boneco_corpo()
  end;
  readln (palavra)
  clrscr;
  if (palavra <> resposta[opcao]) then
  begin
    boneco_braco1()
  end;
  readln (palavra)
  clrscr;
  if (palavra <> resposta[opcao]) then
  begin
    boneco_braco2()
  end;
  readln (palavra)
  clrscr;
  if (palavra <> resposta[opcao]) then
  begin
    boneco()
  end;
  writeln ('VocÃª perdeu')
end;

Begin
  
  resposta[1]:= 'eae';
  resposta[2]:= 'teste';
  
  writeln ('Escolha a primeira ou a segunda pergunta: ');
  readln (opcao);
  
  case opcao of
    1:
    for i:= 1 to length(resposta[opcao]) do
    begin
      teste := '_ ';
      write (teste)
    end;
    for i:= to 6 do
  end;
end;


2:
for i:= 1 to length(resposta[opcao])
do
begin
  teste := '_ ';
  write (teste)
end;

End.
